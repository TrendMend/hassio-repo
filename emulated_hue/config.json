{
  "name": "Emulated HUE",
  "version": "0.2.14",
  "description": "Hue Emulation for Home Assistant",
  "url": "https://github.com/TrendMend/core-1.git",
  "slug": "emulated_hue",
  "stage": "stable",
  "startup": "application",
  "boot": "auto",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": ["config:rw"],
  "host_network": true,
  "environment": {},
  "homeassistant_api": true,
  "options": {
    "verbose": false
  },
  "schema": {
    "http_port": "int?",
    "https_port": "int?",
    "use_default_ports_for_discovery": "bool?",
    "verbose": "bool"
  },
  "image": "ghcr.io/hass-emulated-hue/core"
}
